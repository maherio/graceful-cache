{
    "name": "maherio/graceful-cache",
    "description": "A wrapper for Laravel's Cache Repository to allow for graceful caching.",
    "keywords": ["cache","caching","laravel"],
    "homepage": "http://github.com/maherio/graceful-cache",
    "license": "MIT",
    "authors": [
        {
            "name": "Tony Maher",
            "email": "amaher@alumni.nd.edu"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.0",
        "illuminate/cache": ">=4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "3.*",
        "mockery/mockery": "*@dev"
    },
    "autoload": {
        "classmap": [
            "src/GracefulCache/Repository"
        ],
        "psr-0": {
            "GracefulCache": "src/GracefulCache"
        }
    }
}
